  for (k = 1 ; k <= n; k++) sum += 1/sqrt(((double)k));  /* aka pow(((double)k), -0.5) */
  printf("%.9f\tk^-0.5\n", sum);

  sum = 0.0;
  for (k = 1; k <= n; k++) sum += 1.0/(((double)k)*(((double)k)+1.0));
  printf("%.9f\t1/k(k+1)\n", sum);

  sum = 0.0;
